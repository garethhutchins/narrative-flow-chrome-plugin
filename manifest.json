{
    "name": "Narrative Flow",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Get pages source from a popup",
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": ["tabs", "<all_urls>"],
    "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'"
  
  }